with Ada.Text_Io; use Ada.Text_IO;
with Ada.Integer_Text_IO; use  Ada.Integer_Text_IO;


procedure Falt is
   
   type Integer_Array is array (1..3) of Integer;
   
   type Sum_Array is array (1..4) of Integer_Array;
  
   
   
   
   
   Integers : Integer_Array;
   Sum : Sum_Array;
   Sum_Amount : Integer:=0;
   
begin
  
   
   for I in Sum'Range loop
      
      
      for J in 1..3 loop
	 
	
	Get(Integers(J));
	     
	     end loop;
	 
	 Sum(I):=Integers;
	 
	 
   end loop;
   
   for k in Sum'Range loop
      
      
      for y in Integers'range loop
	 Sum_Amount:= Sum_amount + Integers(y);
	 Put(Integers(Y));
	 
      end loop;
      
      
      end loop;
 
       Put(Sum_Amount);
 	
	 
	 
	 
	 
	 
	 
   
   
   end Falt;
